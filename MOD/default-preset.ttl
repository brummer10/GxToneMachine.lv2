@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://guitarix.sourceforge.net/plugins/gx_foxxtonemachine_#_foxxtonemachine_> ;
	lv2:port [
		lv2:symbol "OCTAVE" ;
		pset:value 0.0
	] , [
		lv2:symbol "SUSTAIN" ;
		pset:value 0.25
	] , [
		lv2:symbol "TONE" ;
		pset:value 0.5
	] , [
		lv2:symbol "VOLUME" ;
		pset:value 0.25
	] .

